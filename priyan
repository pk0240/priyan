#include <stdio.h>  
#include <math.h>  
int main ()  
{  
    int a = 5, b = 10;  
    
    printf (" a == b : %d", (a == b));  
    if (a == b){  
        printf ("\n %d is equal to %d", a, b);  }
    else { 
        printf (" \n %d is not equal to %d", a, b);  }
    
    int x = 5,y = 5;  
     
    printf (" \n x == y : %d", (x == y));  
    if (a == b){  
        printf (" \n %d is equal to %d", x, y);}  
    else { 
        printf ("\n %d is not equal to %d", x, y);  }
    return 0;     
}     
